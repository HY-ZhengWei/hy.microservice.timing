<?xml version="1.0" encoding="UTF-8"?>

<config>

    <import name="xconfig"         class="java.util.ArrayList" />
    <import name="xparam"          class="java.util.Hashtable" />
    <import name="item"            class="org.hy.common.app.Param" />
    
    
    
    <!-- 系统配置参数信息 -->
    <xconfig>

        <xparam id="SYSParam_MS_Timing" key="name">
            
            <item id="MS_Timing_Analyses_Password">
                <name>MS_Timing_Analyses_Password</name>
                <value encrypt="org.hy.common.StringHelp">mstiming</value>
                <comment>分析中心的访问密码</comment>
            </item>
            
            
            <item id="IsInitDSG_MS_Timing">
                <name>IsInitDSG_MS_Timing</name>
                <value>启用</value>
                <comment>是否初始化数据库连接池组。如果不在内部初始化，请在外部预先初始化好</comment>
            </item>
            
            
            <item id="MS_Timing_RootPackageName">
                <name>MS_Timing_RootPackageName</name>
                <value>org.hy.microservice.timing</value>
                <comment>顶级包的路径。用于初始化。一般不修改</comment>
            </item>
            
            
            <item id="MS_Timing_IsCheckToken">
                <name>MS_Timing_IsCheckToken</name>
                <value>false</value>
                <comment>是否验证用户登录的票据</comment>
            </item>
            
            
            <item id="MS_Timing_TPool_MaxThread">
                <name>MS_Timing_TPool_MaxThread</name>
                <value>100</value>
                <comment>最大线程数</comment>
            </item>
            
            
            <item id="MS_Timing_TPool_MinThread">
                <name>MS_Timing_TPool_MinThread</name>
                <value>1</value>
                <comment>最小线程数</comment>
            </item>
            
            
            <item id="MS_Timing_TPool_MinIdleThread">
                <name>MS_Timing_TPool_MinIdleThread</name>
                <value>1</value>
                <comment>最小空闲数</comment>
            </item>
            
            
            <item id="MS_Timing_TPool_IntervalTime">
                <name>MS_Timing_TPool_IntervalTime</name>
                <value>10</value>
                <comment>间隔时间</comment>
            </item>
            
            
            <item id="MS_Timing_TPool_IdleTimeKill">
                <name>MS_Timing_TPool_IdleTimeKill</name>
                <value>600</value>
                <comment>空闲自毁时长</comment>
            </item>
            
            
            <item id="MS_Timing_TPool_WaitResource">
                <name>MS_Timing_TPool_WaitResource</name>
                <value>23</value>
                <comment>等待线程资源的时长(单位：毫秒)</comment>
            </item>
        </xparam>
        
    </xconfig>
    
</config>